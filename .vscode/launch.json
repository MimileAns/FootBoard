{
  "version": "0.2.0",
  "configurations": [  
    {
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/debug/build/Footboard_V7.elf",
      "name": "Debug with ST-Link",
      "request": "launch",
      "type": "cortex-debug",
      "gdbPath": "arm-none-eabi-gdb",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none",
      "servertype": "stlink",
      "interface": "swd",
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      },
    }
  ]
}
